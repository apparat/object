{
    "name": "apparat/object",
    "description": "Apparat object abstraction layer",
    "license": "MIT",
    "authors": [
        {
            "name": "Joschi Kuphal",
            "email": "joschi@kuphal.net"
        }
    ],
    "minimum-stability": "RC",
    "require": {
        "apparat/resource": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "satooshi/php-coveralls": "^0.6"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:apparat/resource.git",
            "reference": "master"
        }
    ],
    "autoload": {
        "psr-4": {
            "Apparat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApparatTest\\": "test/"
        }
    }
}
