{
  "name": "apparat/object",
  "description": "Apparat object abstraction layer",
  "homepage": "https://apparat.tools",
  "minimum-stability": "RC",
  "license": "MIT",
  "authors": [
    {
      "name": "Joschi Kuphal",
      "email": "joschi@kuphal.net",
      "homepage": "https://jkphl.is",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "apparat/resource": "@dev",
    "dominicsayers/isemail": "dev-master",
    "league/commonmark": "^0.13",
    "psr/http-message": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3",
    "satooshi/php-coveralls": "^1.0",
    "codeclimate/php-test-reporter": "dev-master",
    "apparat/dev": "@dev"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:apparat/kernel.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:apparat/resource.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:apparat/dev.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:dominicsayers/isemail.git",
      "reference": "master"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apparat\\": "src/"
    },
    "files": [
      "src/Object/Module.php",
      "src/Object/Autorun.php"
    ]
  }
}
